    for(ci=list.constBegin();ci!=list.constEnd();++ci) //java stay iterator&#13;
    {&#13;
        qDebug()&lt;&lt;*ci;&#13;
    }&#13;
&#13;
    return a.exec();&#13;
}&#13;
