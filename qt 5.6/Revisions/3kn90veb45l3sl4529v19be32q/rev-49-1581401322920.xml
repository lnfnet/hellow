{&#13;
    QCoreApplication a(argc, argv);&#13;
    QList&lt;int&gt; list;&#13;
    for(int j=0;j&lt;10;j++)&#13;
    {&#13;
        list.insert(list.end(),j);&#13;
    }&#13;
    QList&lt;int&gt;::iterator i;&#13;
